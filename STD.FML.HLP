,,}}}
[f_Sastolin]
{{"Функция СТРОКА_РАЗБИТЬ()",,{;
,ПМ_ВЫД("Синтаксис функций","FunctionSynt")+"  "+ПМ_ВЫД("Группа","FuncString");
,;
,ПМ_ВЫД("СТРОКА_РАЗБИТЬ(Стр: строка, Высота: целое, [Длина: целое], [Выровнять: логическое], [УбрПроб: логическое]): строка",ПМ_ЗАГЛ);
,"Разбивает строку "+ПМ_ВЫД("Стр")+" на количество строк "+ПМ_ВЫД("Высота")+" длины "+ПМ_ВЫД("Длина")+". Строка разбивается, по возможности, по словам, если это не удаётся – по слогам, если и это не удаётся – по символам.";
,'Если '+ПМ_ВЫД("Длина")+'='+ПМ_ВЫД('0')+' или не указана , строка разбивается по словам или по слогам до минимально возможной длины при указанном количестве строк.';
,"Если "+ПМ_ВЫД("Длина")+"<"+ПМ_ВЫД("0")+", строка разбивается по словам или по слогам до минимально возможной длины, которая не меньше "+ПМ_ВЫД("–Длина")+", при указанном количестве строк.";
,"Если "+ПМ_ВЫД("Выровнять")+"="+ПМ_ВЫД("ДА")+", полученные строки прибиваются к левому и правому краю. В противном случае, если "+ПМ_ВЫД("УбрПроб")+"="+ПМ_ВЫД("ДА")+", в полученных строках убираются левые и правые пробелы.";
,"См. также "+ПМ_ВЫД("СТРОКА_ЧАСТЬ()","f_Snastolin")+".";
,;
,ПМ_ВЫД("Примеры:");
,ПМ_ВЫД('ПЕРЕМЕННЫЕ',,ПМ_АЛГ)+' А';
,'"Я помню чудное мгновенье: передо мной явилась ты"=>А';
,'СТРОКА_РАЗБИТЬ(А,3,20)';
,ПМ_ВЫД('Возвращает:',ПМ_ПРИМ);
,ПМ_ВЫД('"Я помню чудное      "+ПЕРЕВОД_СТРОКИ+',ПМ_ПРИМ);
,ПМ_ВЫД('"мгновенье: передо   "+ПЕРЕВОД_СТРОКИ+',ПМ_ПРИМ);
,ПМ_ВЫД('"мной явилась ты     "',ПМ_ПРИМ);
,'СТРОКА_РАЗБИТЬ(А,3,20,ДА)';
,ПМ_ВЫД('Возвращает:',ПМ_ПРИМ);
,ПМ_ВЫД('"Я    помню    чудное"+ПЕРЕВОД_СТРОКИ+',ПМ_ПРИМ);
,ПМ_ВЫД('"мгновенье:    передо"+ПЕРЕВОД_СТРОКИ+',ПМ_ПРИМ);
,ПМ_ВЫД('"мной    явилась   ты"',ПМ_ПРИМ);
,'СТРОКА_РАЗБИТЬ(А,3,20,,ДА)';
,ПМ_ВЫД('Возвращает:',ПМ_ПРИМ);
,ПМ_ВЫД('"Я помню чудное"+ПЕРЕВОД_СТРОКИ+',ПМ_ПРИМ);
,ПМ_ВЫД('"мгновенье: передо"+ПЕРЕВОД_СТРОКИ+',ПМ_ПРИМ);
,ПМ_ВЫД('"мной явилась ты"',ПМ_ПРИМ);
,,}}}
[f_SDPart]
{{"Функция ТБЛ_ВЫВЕСТИ()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncSumm');
,;
,ПМ_ВЫД("ТБЛ_ВЫВЕСТИ(Начало: строка, [Шаблоны: массив], [Разделы: массив], [СтрНач: строка|массив], [СтрКон: строка|массив], [ДокНач: любое], [ДокКон: любое], [Индикатор: логическое], [Алг: логическое], [ШаблСтрНачКон: массив], [ОбНеВСтр: массив]): логическое",ПМ_ЗАГЛ);
,'Выводит разделы формы в соответствии с данными таблицы.';
,;
,ПМ_ВЫД('Параметры:');
,ПМ_ВЫД('Начало        ')+'Будут выводиться только строки, начинающиеся на '+ПМ_ВЫД('Начало');
,ПМ_ВЫД('Шаблоны       ')+'Массив '+ПМ_ВЫД('шаблонов','SummShape')+', которым соответствуют разделы формы';
,ПМ_ВЫД('Разделы       ')+'Массив наименований выводимых разделов, соответствующих шаблонам';
,ПМ_ВЫД('СтрНач        ')+'Разделы формы, выводимые в начале каждой страницы (см. '+ПМ_ВЫД('РАЗДЕЛ_НАЧ',,ПМ_АЛГ)+')';
,ПМ_ВЫД('СтрКон        ')+'Разделы формы, выводимые в конце каждой страницы (см. '+ПМ_ВЫД('РАЗДЕЛ_КОН',,ПМ_АЛГ)+')';
,ПМ_ВЫД('ДокНач        ')+'Разделы формы, выводимые в начале документа';
,ПМ_ВЫД('ДокКон        ')+'Разделы формы, выводимые в конце документа';
,ПМ_ВЫД('Индикатор     ')+'Выводить сообщение подобно функции '+ПМ_ВЫД('ДОК_ИНДИКАТОР()',,ПМ_АЛГ);
,ПМ_ВЫД('Алг           ')+'Параметры '+ПМ_ВЫД('ДокНач')+', '+ПМ_ВЫД('ДокКон')+' и элементы параметра '+ПМ_ВЫД('Разделы')+', являющиеся массивами, выступают в роли параметров для выполнения функции '+ПМ_ВЫД('ДОК_АЛГОРИТМ()',,ПМ_АЛГ)+ПМ_ВЫД(' (см. Пример 2)',ПМ_ПРИМ);
,ПМ_ВЫД('ШаблСтрНачКон ')+'Массив описания открывающих и закрывающих страницу разделов';
,'                '+ПМ_ВЫД('СТРУКТУРА',,ПМ_АЛГ)+' СтрШабл,СтрНач,СтрКон';
,'                '+ПМ_ВЫД('1')+' Шаблон';
,'                '+ПМ_ВЫД('2')+' Раздел, открывающий страницу';
,'                '+ПМ_ВЫД('3')+' Раздел, закрывающий страницу';
,ПМ_ВЫД('ОбНеВСтр      ')+'Если указан, в случае автоматического перевода страницы с несколькими разделами, которые должны находиться на одной странице, но по высоте не помещаются на неё, выполняется функция '+ПМ_ВЫД('ДОК_АЛГОРИТМ()',,ПМ_АЛГ)+' с параметрами, описанными в массиве '+ПМ_ВЫД('ОбНеВСтр');
,;
,'Массивы '+ПМ_ВЫД('Шаблоны')+' и '+ПМ_ВЫД('Разделы')+' могут либо оба отсутствовать, либо их размер должен быть одинаковым. В случае их отсутствия выводятся разделы, соответствующие наименованиям строк в таблице. В противном случае для каждой строки таблицы ведётся поиск подходящего шаблона в массиве '+ПМ_ВЫД('Шаблоны')+'. Если шаблон найден, выводится соответствующий раздел из массива '+ПМ_ВЫД('Разделы')+'. Если шаблон не найден, выводится раздел с наименованием строки таблицы.';
,'Если указан '+ПМ_ВЫД('ШаблСтрНачКон')+', то перед выводом каждого раздела будет производиться поиск в '+ПМ_ВЫД('ШаблСтрНачКон[н].СтрШабл')+' шаблона, соответствующего выводимому разделу. Если шаблон найден, то будут устанавливаться открывающий и закрывающий страницу разделы равными '+ПМ_ВЫД('ШаблСтрНачКон[н].СтрНач')+' и '+ПМ_ВЫД('ШаблСтрНачКон[н].СтрКон')+'. В противном случае, или если '+ПМ_ВЫД('ШаблСтрНачКон[н].СтрНач')+' или '+ПМ_ВЫД('ШаблСтрНачКон[н].СтрКон')+' не являются строками, будут устанавливаться открывающий и закрывающий страницу разделы равными '+ПМ_ВЫД('СтрНач')+' и '+ПМ_ВЫД('СтрКон')+'.';
,'Параметры '+ПМ_ВЫД('ДокНач')+', '+ПМ_ВЫД('ДокКон')+' и элементы массива '+ПМ_ВЫД('Разделы')+' могут быть наименованиями разделов или массивами наименований разделов. Вывод разделов в документ описан в функции '+ПМ_ВЫД('ДОК_РАЗДЕЛ()',,ПМ_АЛГ)+'. Дополнительно к этому описанию действует правило для параметров '+ПМ_ВЫД('ДокНач')+', '+ПМ_ВЫД('ДокКон')+' и элементов массива '+ПМ_ВЫД('Разделы')+', являющихся массивами: если '+ПМ_ВЫД('Алг')+'='+ПМ_ВЫД('НЕТ')+', на время их вывода отключается автоматический перевод страницы, т.е. они будут всегда выводится на одной странице; в противном случае вызывается функция '+ПМ_ВЫД('ДОК_АЛГОРИТМ()')+' с параметрами, соответствующими элементам массива.';
,'Если '+ПМ_ВЫД('Индикатор')+'='+ПМ_ВЫД('НЕТ')+' '+ПМ_ВЫД('(по умолчанию ДА)',ПМ_ПРИМ)+', то сообщение о процессе вывода таблицы не будет выводиться.';
,'Если вывод таблицы успешно завершен, возвращает '+ПМ_ВЫД('ДА')+', если вывод был прерван – '+ПМ_ВЫД('НЕТ')+'.';
,;
,ПМ_ВЫД('Пример 1:');
,ПМ_ВЫД('ДОК_ОТКРЫТЬ()',,ПМ_АЛГ);
,ПМ_ВЫД('ЕСЛИ',,ПМ_АЛГ)+' ТБЛ_ВЫВЕСТИ("",;';
,'{"Z"          ,"Z,#"},;';
,'{{"4","6","4"},"5"  },;';
,'"2","4",{"1","2"},"")';
,'  ДОК_ЗАКРЫТЬ()';
,'ИНАЧЕ';
,'  ДОК_ПРЕРВАТЬ()';
,'КОНЕЦ_ЕСЛИ';
,;
,ПМ_ВЫД('Пример 2:');
,ПМ_ВЫД('ПАРАМЕТР',,ПМ_АЛГ)+' алг,пар1';
,ПМ_ВЫД('СТРУКТУРА',,ПМ_АЛГ)+' ДрСтр';
,ПМ_ВЫД('ПЕРЕМЕННЫЕ',,ПМ_АЛГ)+' н';
,ПМ_ВЫД('ЕСЛИ',,ПМ_АЛГ)+' '+ПМ_ВЫД('ТИП',ДА,ПМ_АЛГ)+'(алг)=ТИП_ЦЕЛОЕ';
,'  '+ПМ_ВЫД('ЕСЛИ',,ПМ_АЛГ)+' алг=#ДрСтр';
,'    '+ПМ_ВЫД('ДОК_ДР_СТР()',,ПМ_АЛГ)+', пар1=>'+ПМ_ВЫД('РАЗДЕЛ_НАЧ',,ПМ_АЛГ);
,'  КОНЕЦ_ЕСЛИ';
,'  '+ПМ_ВЫД('ИТОГО',ДА,ПМ_АЛГ)+'(ДА)';
,'КОНЕЦ_ЕСЛИ';
,ПМ_ВЫД('ДОК_ОТКРЫТЬ',ДА,ПМ_АЛГ)+'(), '+ПМ_ВЫД('ТБЛ_ОБНУЛИТЬ()',,ПМ_АЛГ)+', 0=>н';
,ПМ_ВЫД('ПОКА',,ПМ_АЛГ)+' ++н<=30';
,'  '+ПМ_ВЫД('ТБЛ_УСТАНОВИТЬ',ДА,ПМ_АЛГ)+'("D,"+'+ПМ_ВЫД('СТРОКА',ДА,ПМ_АЛГ)+'(н,3)+",","S",1)';
,'КОНЕЦ_ПОКА';
,'ТБЛ_ВЫВЕСТИ("D,",{"D,  1,"=>н},{{{#ДрСтр,"PH1"},н}},,,{{#ДрСтр,"PH2"},"DH"},{{#ДрСтр,"PH3"},"DF"},,ДА)';
,'ДОК_ЗАКРЫТЬ()';
,,}}}
[f_SearchPath]
{{"Функция ФАЙЛ_НАЙТИ()",,{;
