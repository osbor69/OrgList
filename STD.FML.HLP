[f_AAdd]
{{"Функция МАС_ДОБАВИТЬ()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncArray');
,;
,ПМ_ВЫД("МАС_ДОБАВИТЬ(Мас: массив, [Знач: любое], [ВидПоиска: логическое|целое|строка]): массив",ПМ_ЗАГЛ);
,'Возвращает массив '+ПМ_ВЫД('Мас')+' с добавленным в конце элементом '+ПМ_ВЫД('Знач')+'.';
,'Если указан '+ПМ_ВЫД('ВидПоиска')+', добавление производится, если поиск по указанному виду поиска (см. '+ПМ_ВЫД('МАС_ПОИСК()',,ПМ_АЛГ)+', параметр '+ПМ_ВЫД('Вид')+') определяет отсутствие '+ПМ_ВЫД('Знач')+' в '+ПМ_ВЫД('Мас')+', а если '+ПМ_ВЫД('Знач')+' найден, его значение присваивается в элемент '+ПМ_ВЫД('Мас')+' с найденным номером (используется для изменения значения элемента, являющегося массивом при '+ПМ_ВЫД('ВидПоиска')+'='+ПМ_ВЫД('3')+'). Номер добавленного или изменённого элемента сохраняется в переменной '+ПМ_ВЫД('МАС_ДОБНОМЕР',,ПМ_АЛГ)+'.';
,;
,ПМ_ВЫД("Пример:");
,'МАС_ДОБАВИТЬ({1,2,"ABC"},2,"12") '+ПМ_ВЫД('&'+'& Возвращает {1,2,"ABC","12"}',ПМ_ПРИМ);
,,}}}
