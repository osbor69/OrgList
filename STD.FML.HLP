,'ИНАЧЕ';
,'  '+ПМ_ВЫД('ЕСЛИ',,ПМ_АЛГ)+' алг=="ПР"';
,'    '+ПМ_ВЫД('ИТОГО',ДА,ПМ_АЛГ)+'('+ПМ_ВЫД('ПРОБЕЛЫ_УБРАТЬ',ДА,ПМ_АЛГ)+'(пар1)=='+ПМ_ВЫД('ПРОБЕЛЫ_УБРАТЬ',ДА,ПМ_АЛГ)+'(пар2))';
,'  КОНЕЦ_ЕСЛИ';
,'КОНЕЦ_ЕСЛИ';
,;
,'См. также '+ПМ_ВЫД('Логические операторы с двумя выражениями','И',ПМ_АЛГ)+'.';
