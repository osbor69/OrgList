,ПМ_ВЫД('&'+'& ,{5.2.8,ДАТА_МАКС,{20,"T"}}}',ПМ_ПРИМ);
,'ДДАН_ИЗМЕН({})';
,ПМ_ВЫД('&'+'& Возвращает:',ПМ_ПРИМ);
