{{"Функция МАС_ЗАПОЛНИТЬ()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncArray');
,;
,ПМ_ВЫД("МАС_ЗАПОЛНИТЬ(Мас: массив|целое, [Заполн: любое], [Начало: целое], [Кол: целое]): массив",ПМ_ЗАГЛ);
,'Заполняет массив '+ПМ_ВЫД('Мас')+' значением '+ПМ_ВЫД('Заполн')+', начиная с элемента с номером '+ПМ_ВЫД('Начало')+' '+ПМ_ВЫД('(по умолчанию 1)',ПМ_ПРИМ)+' длиной '+ПМ_ВЫД('Кол')+' '+ПМ_ВЫД('(по умолчанию заполняется до конца массива)',ПМ_ПРИМ)+'. Если '+ПМ_ВЫД('Мас')+' явлется целым, он заменяется массивом длиной '+ПМ_ВЫД('Мас')+' аналогично функции '+ПМ_ВЫД('МАССИВ',ДА,ПМ_АЛГ)+'('+ПМ_ВЫД('Мас')+').';
,;
,ПМ_ВЫД("Примеры:");
,'МАС_ЗАПОЛНИТЬ({12,3,5,3,11,6},0,2,3) '+ПМ_ВЫД('&'+'& Возвращает {12,0,0,0,11,6}',ПМ_ПРИМ);
,'МАС_ЗАПОЛНИТЬ(4,0)                   '+ПМ_ВЫД('&'+'& Возвращает {0,0,0,0}',ПМ_ПРИМ);
,,}}}
[f_AIf]
{{"Функция МАС_ВЫБОР()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncArray');
,;
,ПМ_ВЫД("МАС_ВЫБОР([Усл: любое], МасУсл: массив, МасЗн: массив, [Умолч: любое]): любое",ПМ_ЗАГЛ);
,'Производит поиск значения '+ПМ_ВЫД('Усл')+' '+ПМ_ВЫД('(по умолчанию ДА)',ПМ_ПРИМ)+' в массиве '+ПМ_ВЫД('МасУсл')+'. Если такой элемент найден, возвращает значение соответствующего элемента массива '+ПМ_ВЫД('МасЗн')+'. В противном случае возвращает '+ПМ_ВЫД('Умолч')+'.';
,;
,ПМ_ВЫД("Примеры:");
,'МАС_ВЫБОР(4,{12,4,5},{"vv","e","df"})    '+ПМ_ВЫД('&'+'& Возвращает "e"',ПМ_ПРИМ);
,'МАС_ВЫБОР(3,{12,4,5},{"vv","e","df"},"") '+ПМ_ВЫД('&'+'& Возвращает ""',ПМ_ПРИМ);
,,}}}
[f_AIns]
{{"Функция МАС_ВСТАВИТЬ()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncArray');
,;
,ПМ_ВЫД("МАС_ВСТАВИТЬ(Мас: массив, Номер: целое, [Знач: любое]): массив",ПМ_ЗАГЛ);
,'Возвращает массив '+ПМ_ВЫД('Мас')+' с добавленным в него элементом '+ПМ_ВЫД('Знач')+' перед элементом с номером '+ПМ_ВЫД('Номер')+'.';
,;
,ПМ_ВЫД("Пример:");
,'МАС_ВСТАВИТЬ({1,2,"ABC"},2,"12") '+ПМ_ВЫД('&'+'& Возвращает {1,"12",2,"ABC"}',ПМ_ПРИМ);
,,}}}
[f_ARaspr]
{{"Функция МАС_РАСПРЕД()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncArray');
,;
,ПМ_ВЫД("МАС_РАСПРЕД(Мас: массив, Точн: целое, Сумма: число, [Оптим: логическое], [Знак: логическое]): массив",ПМ_ЗАГЛ);
,'Если '+ПМ_ВЫД('Сумма')+' является числом, оно распределяется по массиву '+ПМ_ВЫД('Мас')+' с точностью '+ПМ_ВЫД('Точн')+' пропорционально элементам массива.';
,'Если '+ПМ_ВЫД('Оптим')+'='+ПМ_ВЫД('ДА')+', погрешность округления распределяется по наилучшим по критерию округления элементам '+ПМ_ВЫД('Мас')+'.';
,'Если '+ПМ_ВЫД('Знак')+'='+ПМ_ВЫД('ДА')+', элементы массива со знаком, противоположным знаку '+ПМ_ВЫД('Сумма')+', рассматриваются как равные '+ПМ_ВЫД('0')+'.';
,;
,ПМ_ВЫД("Пример:");
,'МАС_РАСПРЕД({1,2,3,4},2,220.4) '+ПМ_ВЫД('&'+'& Возвращает 3',ПМ_ПРИМ);
,;
,ПМ_ВЫД("МАС_РАСПРЕД(Мас: массив, Точн: целое, Сумма: массив, [Приор: массив]): массив",ПМ_ЗАГЛ);
,'Если '+ПМ_ВЫД('Сумма')+' является массивом, производится распределение сумм, содержащихся в '+ПМ_ВЫД('Сумма')+', по условиям, заданным в '+ПМ_ВЫД('Мас')+', являющимся двумерным массивом.';
,'Если обозначить результат как '+ПМ_ВЫД('Рез')+', и присвоить:';
,'  ДЛИНА('+ПМ_ВЫД('Сумма')+') => '+ПМ_ВЫД('Y');
,'  ДЛИНА('+ПМ_ВЫД('Мас[1]')+') => '+ПМ_ВЫД('X');
,'то должны выполняться следующие равенства';
,'  ДЛИНА('+ПМ_ВЫД('Мас')+') == '+ПМ_ВЫД('Y');
,'  ДЛИНА('+ПМ_ВЫД('Рез')+') == '+ПМ_ВЫД('X');
,'  ДЛИНА('+ПМ_ВЫД('Приор')+') == '+ПМ_ВЫД('X')+' '+ПМ_ВЫД('&'+'& Если Приор указан',ПМ_ПРИМ);
,'Каждый элемент '+ПМ_ВЫД('Мас')+' ('+ПМ_ВЫД('МасY')+') соответствует элементу '+ПМ_ВЫД('Сумма')+' ('+ПМ_ВЫД('СуммаY')+') и является массивом чисел, задающих вес соответствующего элемента '+ПМ_ВЫД('Рез')+' ('+ПМ_ВЫД('РезX')+'), в который распределяется '+ПМ_ВЫД('СуммаY')+'. Если какой-либо вес не указан, соответствующий '+ПМ_ВЫД('РезX')+' не участвует в распределении '+ПМ_ВЫД('СуммаY')+'. Распределение производится с точностью '+ПМ_ВЫД('Точн')+'.';
,'Если указан '+ПМ_ВЫД('Приор')+', каждый его элемент ('+ПМ_ВЫД('ПриорX')+') соответствует приоритету '+ПМ_ВЫД('РезX')+'. Распределение производится отдельно для элементов '+ПМ_ВЫД('Рез')+' одного приоритета по мере возрастания приоритета. Если приоритет '+ПМ_ВЫД('РезX')+' отрицательный или не указан, он не участвует в распределении.';
,'При невозможности соблюдения равенства '+ПМ_ВЫД('СуммаY')+' сумме элементов соответствующего '+ПМ_ВЫД('МасY')+' ('+ПМ_ВЫД('МасYX')+') соблюдается условие '+ПМ_ВЫД('МасYX')+' <= '+ПМ_ВЫД('СуммаY')+'.';
,;
,ПМ_ВЫД("Пример:");
,'МАС_РАСПРЕД(;';
,'{{500   ,       ,3000  ,};';
,',{      ,500    ,      ,3000};';
,',{654.83,2845.17,      ,};';
,',{      ,       ,654.83,2845.17};';
,'},2;';
,',{654.83,2845.17,500   ,3000})';
,ПМ_ВЫД('Возвращает',ПМ_ПРИМ);
,ПМ_ВЫД(' {93.55 ,406.45 ,561.28,2438.72}',ПМ_ПРИМ);
,,}}}
[f_Array]
{{"Функция МАССИВ()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncArray');
,;
,ПМ_ВЫД("МАССИВ(Разм1: целое, [Разм2: целое], [Разм3: целое], [Разм4: целое], [Разм5: целое]): массив",ПМ_ЗАГЛ);
,'Возвращает массив указанной размерности. Массив может иметь от '+ПМ_ВЫД('1')+' до '+ПМ_ВЫД('5')+' уровней вложенности. Размерность каждого уровня указывается параметрами '+ПМ_ВЫД('Разм1')+' - '+ПМ_ВЫД('Разм5')+'.';
,,}}}
[f_ArrayAsList]
{{"Функция МАССИВ_СТРОКА()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncString');
,;
,ПМ_ВЫД("МАССИВ_СТРОКА(Мас: массив, [Рзд: строка], [Посл: логическое]): строка",ПМ_ЗАГЛ);
,'Преобразует массив '+ПМ_ВЫД('Мас')+' в строку с разделителями '+ПМ_ВЫД('Рзд')+' '+ПМ_ВЫД('(по умолчанию ",")',ПМ_ПРИМ)+'.';
,'Если '+ПМ_ВЫД('Посл')+'='+ПМ_ВЫД('ДА')+' '+ПМ_ВЫД('(по умолчанию)',ПМ_ПРИМ)+', в конце строки добавляется разделитель '+ПМ_ВЫД('Рзд')+'.';
,;
,ПМ_ВЫД("Примеры:");
,'МАССИВ_СТРОКА({"12","45","3"},"/")     '+ПМ_ВЫД('&'+'& Возвращает "12/45/3/"',ПМ_ПРИМ);
,'МАССИВ_СТРОКА({"12","45","3"},"/",НЕТ) '+ПМ_ВЫД('&'+'& Возвращает "12/45/3"',ПМ_ПРИМ);
,;
,'См. также функцию '+ПМ_ВЫД('СТРОКА_МАССИВ()',,ПМ_АЛГ)+'.';
,,}}}
[f_ArrCondMatrix]
{{"Функция ТБЛ_УСЛСУМ()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncSumm');
,;
,ПМ_ВЫД("ТБЛ_УСЛСУМ(Стр: строка|массив, Столб: строка|массив, [РезСтр: строка], [РезСтолб: строка], [ВтчСтр: логическое], [ВтчСтолб: логическое], [Пров: логическое], [Точн: целое], [Приор: целое]): массив|строка",ПМ_ЗАГЛ);
,'Задаёт дополнительные условия суммирования для функции '+ПМ_ВЫД('ТБЛ_ОКРУГЛ()',,ПМ_АЛГ)+'.';
,'Параметры '+ПМ_ВЫД('Стр')+' и '+ПМ_ВЫД('Столб')+' описывают строки и столбцы таблицы, которые будут суммироваться в строку '+ПМ_ВЫД('РезСтр')+' '+ПМ_ВЫД('(если она указана)',ПМ_ПРИМ)+' и в столбец '+ПМ_ВЫД('РезСтолб')+' '+ПМ_ВЫД('(если он указан)',ПМ_ПРИМ)+'. Один из параметров '+ПМ_ВЫД('РезСтр')+' и '+ПМ_ВЫД('РезСтолб')+' должен быть обязательно указан. Если '+ПМ_ВЫД('Стр')+' или '+ПМ_ВЫД('Столб')+' является '+ПМ_ВЫД('шаблоном','SummShape')+', то учитываются строки или столбцы, соответствующие этому шаблону.';
,'Параметры '+ПМ_ВЫД('РезСтр')+' и '+ПМ_ВЫД('РезСтолб')+' могут являться шаблонами. В таком случае '+ПМ_ВЫД('Стр')+' или '+ПМ_ВЫД('Столб')+' должны быть шаблонами аналогично функциям '+ПМ_ВЫД('ТБЛ_СУМ_СТР()',,ПМ_АЛГ)+' и '+ПМ_ВЫД('ТБЛ_СУМ_СТОЛБ()',,ПМ_АЛГ)+'.';
,'Параметры '+ПМ_ВЫД('ВтчСтр')+' и '+ПМ_ВЫД('ВтчСтолб')+' определяют, является ли строка/столбец результата суммой исходных строк/столбцов '+ПМ_ВЫД('(НЕТ)',ПМ_ПРИМ)+', либо они входят в неё в том числе '+ПМ_ВЫД('(ДА)',ПМ_ПРИМ)+'. В последнем случае добавляется невидимая строка/столбец, которая является суммой исходных строк/столбцов и условно равняется строке/столбцу результата (см. '+ПМ_ВЫД('ТБЛ_ОКРУГЛ()',,ПМ_АЛГ)+', параметр '+ПМ_ВЫД('Равно')+').';
,'Результат используется в качестве параметра '+ПМ_ВЫД('УслСум')+' в функции '+ПМ_ВЫД('ТБЛ_ОКРУГЛ()',,ПМ_АЛГ)+'.';
,'Если '+ПМ_ВЫД('Пров')+'='+ПМ_ВЫД('ДА')+', то суммы значений из ячеек, описанных в '+ПМ_ВЫД('Стр')+' и '+ПМ_ВЫД('Столб')+', сравниваются со значениями ячеек, описанных в '+ПМ_ВЫД('РезСтр')+' и '+ПМ_ВЫД('РезСтолб')+'. В случае несоответствия возвращается строка ошибки.';
,'Суммирование производится с точностью '+ПМ_ВЫД('Точн')+' '+ПМ_ВЫД('(по умолчанию – с максимальной)',ПМ_ПРИМ)+'.';
,'Параметр '+ПМ_ВЫД('Приор')+' устанавливает приоритет для описываемых значений (соответствует элементу '+ПМ_ВЫД('ОкрПриор')+' параметра '+ПМ_ВЫД('Знач')+' функции '+ПМ_ВЫД('ТБЛ_ОКРУГЛ()',,ПМ_АЛГ)+').';
,,}}}
[f_ArrCondRound]
{{"Функция ТБЛ_ОКРУГЛ()",,{;
,ПМ_ВЫД('Синтаксис функций','FunctionSynt')+'  '+ПМ_ВЫД('Группа','FuncSumm');
,;
,ПМ_ВЫД("ТБЛ_ОКРУГЛ(Знач: массив, [Сум: массив], [Равно: массив], [НеИспольз: пусто], [УслСум: массив], [Точн: целое]): массив|пусто",ПМ_ЗАГЛ);
,'Округляет до целых значения, хранимые в массиве '+ПМ_ВЫД('Знач')+', в соответствии с условиями, заданными в '+ПМ_ВЫД('Знач')+', '+ПМ_ВЫД('Сум')+' и '+ПМ_ВЫД('Равно')+'.';
,;
,'Структура элемента массива '+ПМ_ВЫД('Знач')+':';
,ПМ_ВЫД('СТРУКТУРА',,ПМ_АЛГ)+' ОкрЗнач,ОкрВид,ОкрНе0,ОкрСтр,ОкрСтолб,ОкрПриор';
,ПМ_ВЫД('№        Тип         Описание',ПМ_ЗАГЛ);
,ПМ_ВЫД('ОкрЗнач  ')+'число       Округляемое значение (по умолчанию считывается из строки '+ПМ_ВЫД('ОкрСтр')+' и столбца '+ПМ_ВЫД('ОкрСтолб')+' текущей таблицы)';
,ПМ_ВЫД('ОкрВид   ')+'логическое| Если '+ПМ_ВЫД('ДА')+' – правильное округление аналогично функции '+ПМ_ВЫД('ОКРУГЛИТЬ()',,ПМ_АЛГ);
,'         число|      Округляется до данного числа';
,'         строка      '+ПМ_ВЫД('"1"')+' – аналогично '+ПМ_ВЫД('ДА');
,'                     '+ПМ_ВЫД('"2"')+' – по возможности правильное округление '+ПМ_ВЫД('(если не противоречит другим условиям)',ПМ_ПРИМ);
,'                     '+ПМ_ВЫД('"3"')+' – допустима смена знака при округлении';
,ПМ_ВЫД('ОкрНе0   ')+'любое       Не пусто – ненулевое округляемое значение не должно округляться в '+ПМ_ВЫД('0');
,ПМ_ВЫД('ОкрСтр   ')+'строка      Наименование строки ячейки таблицы, куда будет записываться округлённое значение '+ПМ_ВЫД('(по умолчанию не записывается в таблицу)',ПМ_ПРИМ);
,ПМ_ВЫД('ОкрСтолб ')+'строка      Наименование столбца ячейки таблицы, куда будет записываться округлённое значение '+ПМ_ВЫД('(по умолчанию не записывается в таблицу)',ПМ_ПРИМ);
,ПМ_ВЫД('ОкрПриор ')+'целое       Приоритет '+ПМ_ВЫД('(по умолчанию 0)',ПМ_ПРИМ);
,;
,'Если элемент массива '+ПМ_ВЫД('Знач')+' не является массивом, он рассматривается как округляемое значение.';
,'Массив '+ПМ_ВЫД('Сум')+' состоит из массивов коэффициентов, сумма произведений которых на соответствующие округляемые значения должна быть равна '+ПМ_ВЫД('0')+'. Например, если заданы значения {'+ПМ_ВЫД('1.3')+','+ПМ_ВЫД('4.5')+','+ПМ_ВЫД('5.8')+'} '+ПМ_ВЫД('(3-й элемент является суммой первых двух)',ПМ_ПРИМ)+', то '+ПМ_ВЫД('Сум')+'={'+ПМ_ВЫД('1')+','+ПМ_ВЫД('1')+','+ПМ_ВЫД('-1')+'}: '+ПМ_ВЫД('1.3')+'*'+ПМ_ВЫД('1')+' + '+ПМ_ВЫД('4.5')+'*'+ПМ_ВЫД('1')+' + '+ПМ_ВЫД('5.8')+'*'+ПМ_ВЫД('-1')+' = '+ПМ_ВЫД('0')+'.';
,'Массив '+ПМ_ВЫД('Равно')+' состоит из элементов, каждый из которых является массивом минимум из '+ПМ_ВЫД('2')+'-х элементов, описывающих номера округляемых значений, на которые закладываются определённые ограничения, описываемые '+ПМ_ВЫД('3')+'-м необязательным элементом (обозначим как '+ПМ_ВЫД('ТипОгр')+'). Если обозначить округляемые значения в указанной паре как '+ПМ_ВЫД('N1')+' и '+ПМ_ВЫД('N2')+', а соответствующие округлённые значения как '+ПМ_ВЫД('I1')+' и '+ПМ_ВЫД('I2')+', то можно описать следующие ограничения:';
,ПМ_ВЫД('До округления После округления',ПМ_ЗАГЛ);
,ПМ_ВЫД('  ТипОгр = 0 ',ПМ_ЗАГЛ)+ПМ_ВЫД('(по умолчанию)',ПМ_ПРИМ);
,ПМ_ВЫД('N1')+' = '+ПМ_ВЫД('N2')+'       '+ПМ_ВЫД('I1')+' = '+ПМ_ВЫД('I2');
,ПМ_ВЫД('N1')+' < '+ПМ_ВЫД('N2')+'       '+ПМ_ВЫД('I1')+' <= '+ПМ_ВЫД('I2');
,ПМ_ВЫД('N1')+' > '+ПМ_ВЫД('N2')+'       '+ПМ_ВЫД('I1')+' >= '+ПМ_ВЫД('I2');
,ПМ_ВЫД('  ТипОгр = 1',ПМ_ЗАГЛ);
,ПМ_ВЫД('N1')+' = '+ПМ_ВЫД('N2')+'       '+ПМ_ВЫД('I1')+' = '+ПМ_ВЫД('I2');
,ПМ_ВЫД('N1')+' < '+ПМ_ВЫД('N2')+'       '+ПМ_ВЫД('I1')+' < '+ПМ_ВЫД('I2');
,ПМ_ВЫД('N1')+' > '+ПМ_ВЫД('N2')+'       '+ПМ_ВЫД('I1')+' > '+ПМ_ВЫД('I2');
,'Если '+ПМ_ВЫД('ТипОгр')+'='+ПМ_ВЫД('2')+', то ограничения, аналогичные '+ПМ_ВЫД('ТипОгр')+'='+ПМ_ВЫД('0')+', действуют по возможности, если они не противоречат другим условиям '+ПМ_ВЫД('(т.е. ограничения снимаются, если при решении не выполнилось ни одного округления)',ПМ_ПРИМ)+'.';
